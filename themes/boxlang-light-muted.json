{
    "name": "BoxLang Light (Muted)",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#CD853F",
        "variable.readonly": "#D2691E",
        "variable.local": "#BC8F8F",
        "parameter": "#4682B4",
        "function": "#483D8B",
        "method": "#483D8B",
        "class": "#8B008B",
        "interface": "#9370DB",
        "enum": "#B8860B",
        "namespace": "#8FBC8F",
        "type": "#DAA520",
        "keyword": "#B22222",
        "operator": "#CD5C5C",
        "string": "#6B8E23",
        "number": "#D2B48C",
        "boolean": "#F4A460",
        "comment": "#708090"
    },
    "colors": {
        "editor.background": "#FEFEFE",
        "editor.foreground": "#2F4F4F",
        "editorLineNumber.foreground": "#A9A9A9",
        "editorLineNumber.activeForeground": "#CD853F",
        "editor.selectionBackground": "#CD853F40",
        "editor.selectionHighlightBackground": "#CD853F20",
        "editor.lineHighlightBackground": "#F8F8FF",
        "editorCursor.foreground": "#CD853F",
        "editorWhitespace.foreground": "#D3D3D3",
        "editorIndentGuide.background": "#E6E6FA",
        "editorIndentGuide.activeBackground": "#CD853F",
        "editor.findMatchBackground": "#CD853F60",
        "editor.findMatchHighlightBackground": "#CD853F30",
        "editor.wordHighlightBackground": "#CD853F20",
        "editor.wordHighlightStrongBackground": "#CD853F40",
        "editorBracketMatch.background": "#CD853F40",
        "editorBracketMatch.border": "#CD853F",
        "editorBracketHighlight.foreground1": "#CD853F",
        "editorBracketHighlight.foreground2": "#483D8B",
        "editorBracketHighlight.foreground3": "#4682B4",
        "editorBracketHighlight.foreground4": "#8B008B",
        "activityBar.background": "#FEFEFE",
        "activityBar.foreground": "#2F4F4F",
        "activityBar.inactiveForeground": "#A9A9A9",
        "activityBar.border": "#E6E6FA",
        "activityBarBadge.background": "#CD853F",
        "activityBarBadge.foreground": "#FFFFFF",
        "sideBar.background": "#F8F8FF",
        "sideBar.foreground": "#2F4F4F",
        "sideBar.border": "#E6E6FA",
        "sideBarTitle.foreground": "#CD853F",
        "sideBarSectionHeader.background": "#E6E6FA",
        "sideBarSectionHeader.foreground": "#2F4F4F",
        "list.activeSelectionBackground": "#CD853F40",
        "list.activeSelectionForeground": "#2F4F4F",
        "list.inactiveSelectionBackground": "#CD853F20",
        "list.hoverBackground": "#CD853F30",
        "list.focusBackground": "#CD853F40",
        "statusBar.background": "#FEFEFE",
        "statusBar.foreground": "#2F4F4F",
        "statusBar.border": "#E6E6FA",
        "statusBarItem.activeBackground": "#CD853F40",
        "statusBarItem.hoverBackground": "#CD853F30",
        "titleBar.activeBackground": "#FEFEFE",
        "titleBar.activeForeground": "#2F4F4F",
        "titleBar.inactiveBackground": "#FEFEFE",
        "titleBar.inactiveForeground": "#A9A9A9",
        "titleBar.border": "#E6E6FA",
        "tab.activeBackground": "#FEFEFE",
        "tab.activeForeground": "#CD853F",
        "tab.inactiveBackground": "#F8F8FF",
        "tab.inactiveForeground": "#A9A9A9",
        "tab.border": "#E6E6FA",
        "tab.activeBorder": "#CD853F",
        "panel.background": "#F8F8FF",
        "panel.border": "#E6E6FA",
        "panelTitle.activeForeground": "#CD853F",
        "panelTitle.inactiveForeground": "#A9A9A9",
        "terminal.background": "#FEFEFE",
        "terminal.foreground": "#2F4F4F",
        "terminalCursor.background": "#FEFEFE",
        "terminalCursor.foreground": "#CD853F"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#708090"
            }
        },
        {
            "name": "Identifiers",
            "scope": [
                "identifier",
                "variable.other",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#8FBC8F"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#CD853F"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#2F4F4F"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#DC143C"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#B22222"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#CD5C5C"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#483D8B"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#483D8B"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#CD853F"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#D2691E"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#D2B48C"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#6B8E23"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#8B008B"
            }
        },
        {
            "name": "Entity Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#DAA520"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#4682B4"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#8B008B"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#DAA520"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#4682B4"
            }
        }
    ]
}