{
    "name": "BoxLang Dark (Synthwave)",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#FF6B35",
        "variable.readonly": "#FF9A70",
        "variable.local": "#FF8C69",
        "parameter": "#00FFFF",
        "function": "#FF1493",
        "method": "#FF1493",
        "class": "#DA70D6",
        "interface": "#DDA0DD",
        "enum": "#FFD700",
        "namespace": "#00CED1",
        "type": "#7FFF00",
        "keyword": "#FF69B4",
        "operator": "#FF6347",
        "string": "#00FA9A",
        "number": "#FFA500",
        "boolean": "#00BFFF",
        "comment": "#6B7280"
    },
    "colors": {
        "editor.background": "#0B0326",
        "editor.foreground": "#E6E6FF",
        "editorLineNumber.foreground": "#5B5A70",
        "editorLineNumber.activeForeground": "#FF6BCA",
        "editor.selectionBackground": "#7C5CFA33",
        "editor.selectionHighlightBackground": "#7C5CFA22",
        "editor.lineHighlightBackground": "#100528",
        "editorCursor.foreground": "#FF6BCA",
        "editorWhitespace.foreground": "#2A2540",
        "editorIndentGuide.background": "#211A36",
        "editorIndentGuide.activeBackground": "#FF6BCA",
        "editor.findMatchBackground": "#FF6BCA66",
        "editor.findMatchHighlightBackground": "#FF6BCA33",
        "editor.wordHighlightBackground": "#FF6BCA22",
        "editor.wordHighlightStrongBackground": "#FF6BCA33",
        "editorBracketMatch.background": "#FF6BCA33",
        "editorBracketMatch.border": "#FF6BCA",
        "editorBracketHighlight.foreground1": "#FF6BCA",
        "editorBracketHighlight.foreground2": "#7C5CFA",
        "editorBracketHighlight.foreground3": "#06B6D4",
        "editorBracketHighlight.foreground4": "#34D399",
        "activityBar.background": "#07021b",
        "activityBar.foreground": "#E6E6FF",
        "activityBar.inactiveForeground": "#7B7A8A",
        "activityBar.border": "#211A36",
        "activityBarBadge.background": "#FF6BCA",
        "activityBarBadge.foreground": "#0B0326",
        "sideBar.background": "#0f0730",
        "sideBar.foreground": "#E6E6FF",
        "sideBar.border": "#211A36",
        "sideBarTitle.foreground": "#FF6BCA",
        "sideBarSectionHeader.background": "#211A36",
        "sideBarSectionHeader.foreground": "#E6E6FF",
        "list.activeSelectionBackground": "#FF6BCA33",
        "list.activeSelectionForeground": "#FFFFFF",
        "list.inactiveSelectionBackground": "#FF6BCA22",
        "list.hoverBackground": "#FF6BCA22",
        "list.focusBackground": "#FF6BCA33",
        "statusBar.background": "#07021b",
        "statusBar.foreground": "#E6E6FF",
        "statusBar.border": "#211A36",
        "statusBarItem.activeBackground": "#FF6BCA33",
        "statusBarItem.hoverBackground": "#FF6BCA22",
        "titleBar.activeBackground": "#07021b",
        "titleBar.activeForeground": "#E6E6FF",
        "titleBar.inactiveBackground": "#07021b",
        "titleBar.inactiveForeground": "#7B7A8A",
        "titleBar.border": "#211A36",
        "tab.activeBackground": "#0f0730",
        "tab.activeForeground": "#FF6BCA",
        "tab.inactiveBackground": "#07021b",
        "tab.inactiveForeground": "#7B7A8A",
        "tab.border": "#211A36",
        "tab.activeBorder": "#FF6BCA",
        "panel.background": "#0f0730",
        "panel.border": "#211A36",
        "panelTitle.activeForeground": "#FF6BCA",
        "panelTitle.inactiveForeground": "#7B7A8A",
        "terminal.background": "#07021b",
        "terminal.foreground": "#E6E6FF",
        "terminalCursor.background": "#07021b",
        "terminalCursor.foreground": "#FF6BCA"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6B7280"
            }
        },
        {
            "name": "Identifiers",
            "scope": [
                "identifier",
                "variable.other",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#FF9AD0"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "variable.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#7C5CFA",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#06B6D4"
            }
        }
    ]
}