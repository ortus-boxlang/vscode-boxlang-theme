{
    "name": "BoxLang Light (Synthwave)",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#FF6B35",
        "variable.readonly": "#00CED1", 
        "variable.local": "#FF8C69",
        "parameter": "#4A90E2",
        "function": "#00688B",
        "method": "#00688B",
        "class": "#2E8B57",
        "interface": "#20B2AA",
        "enum": "#DAA520",
        "namespace": "#008B8B",
        "type": "#228B22",
        "keyword": "#8B008B",
        "operator": "#FF6347",
        "string": "#006400",
        "number": "#B8860B",
        "boolean": "#4682B4",
        "comment": "#708090"
    },
    "colors": {
        "editor.background": "#FAFAFA",
        "editor.foreground": "#2D3748",
        "editorLineNumber.foreground": "#A0AEC0",
        "editorLineNumber.activeForeground": "#FF6B35",
        "editor.selectionBackground": "#00CED140",
        "editor.selectionHighlightBackground": "#00CED120",
        "editor.lineHighlightBackground": "#F7FAFC",
        "editorCursor.foreground": "#FF6B35",
        "editorWhitespace.foreground": "#E2E8F0",
        "editorIndentGuide.background": "#E2E8F0",
        "editorIndentGuide.activeBackground": "#FF6B35",
        "editor.findMatchBackground": "#FF6B3560",
        "editor.findMatchHighlightBackground": "#FF6B3530",
        "editor.wordHighlightBackground": "#00CED120",
        "editor.wordHighlightStrongBackground": "#00CED140",
        "editorBracketMatch.background": "#FF6B3520",
        "editorBracketMatch.border": "#FF6B35",
        "activityBar.background": "#FBFBFF",
        "activityBar.foreground": "#0F172A",
        "activityBar.inactiveForeground": "#94A3B8",
        "activityBar.border": "#EEF2FF",
        "activityBarBadge.background": "#D946EF",
        "activityBarBadge.foreground": "#FBFBFF",
        "sideBar.background": "#F8F7FF",
        "sideBar.foreground": "#0F172A",
        "sideBar.border": "#EEF2FF",
        "sideBarTitle.foreground": "#D946EF",
        "sideBarSectionHeader.background": "#EEF2FF",
        "sideBarSectionHeader.foreground": "#0F172A",
        "list.activeSelectionBackground": "#D946EF33",
        "list.activeSelectionForeground": "#0F172A",
        "list.inactiveSelectionBackground": "#D946EF22",
        "list.hoverBackground": "#D946EF22",
        "list.focusBackground": "#D946EF33",
        "statusBar.background": "#FBFBFF",
        "statusBar.foreground": "#0F172A",
        "statusBar.border": "#EEF2FF",
        "statusBarItem.activeBackground": "#D946EF33",
        "statusBarItem.hoverBackground": "#D946EF22",
        "titleBar.activeBackground": "#FBFBFF",
        "titleBar.activeForeground": "#0F172A",
        "titleBar.inactiveBackground": "#FBFBFF",
        "titleBar.inactiveForeground": "#94A3B8",
        "titleBar.border": "#EEF2FF",
        "tab.activeBackground": "#F8F7FF",
        "tab.activeForeground": "#D946EF",
        "tab.inactiveBackground": "#FBFBFF",
        "tab.inactiveForeground": "#94A3B8",
        "tab.border": "#EEF2FF",
        "tab.activeBorder": "#D946EF",
        "panel.background": "#F8F7FF",
        "panel.border": "#EEF2FF",
        "panelTitle.activeForeground": "#D946EF",
        "panelTitle.inactiveForeground": "#94A3B8",
        "terminal.background": "#FBFBFF",
        "terminal.foreground": "#0F172A",
        "terminalCursor.foreground": "#D946EF"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#6B7280"
            }
        },
        {
            "name": "Identifiers",
            "scope": [
                "identifier",
                "variable.other",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#0F172A"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "variable.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#4338CA",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#0891B2"
            }
        }
    ]
}