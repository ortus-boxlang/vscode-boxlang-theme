{
    "name": "BoxLang Light (Muted)",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#2E8B57",
        "variable.readonly": "#20B2AA",
        "variable.local": "#5F9EA0",
        "parameter": "#06B6D4",
        "function": "#7C5CFA",
        "method": "#7C5CFA",
        "class": "#6A5ACD",
        "interface": "#9370DB",
        "enum": "#FF6B35",
        "namespace": "#00CED1",
        "type": "#483D8B",
        "keyword": "#8B008B",
        "operator": "#2E8B57",
        "string": "#228B22",
        "number": "#DD35FF",
        "boolean": "#4682B4",
        "comment": "#708090"
    },
    "colors": {
        "editor.background": "#F0FFFE",
        "editor.foreground": "#2F4F4F",
        "editorLineNumber.foreground": "#5F9EA0",
        "editorLineNumber.activeForeground": "#2E8B57",
        "editor.selectionBackground": "#2E8B5740",
        "editor.selectionHighlightBackground": "#2E8B5720",
        "editor.lineHighlightBackground": "#F0FFFF",
        "editorCursor.foreground": "#2E8B57",
        "editorWhitespace.foreground": "#B0E0E6",
        "editorIndentGuide.background": "#E0FFFF",
        "editorIndentGuide.activeBackground": "#2E8B57",
        "editor.findMatchBackground": "#00CED160",
        "editor.findMatchHighlightBackground": "#00CED130",
        "editor.wordHighlightBackground": "#00CED120",
        "editor.wordHighlightStrongBackground": "#00CED140",
        "editorBracketMatch.background": "#2E8B5740",
        "editorBracketMatch.border": "#2E8B57",
        "editorBracketHighlight.foreground1": "#2E8B57",
        "editorBracketHighlight.foreground2": "#7C5CFA",
        "editorBracketHighlight.foreground3": "#06B6D4",
        "editorBracketHighlight.foreground4": "#FF6B35",
        "activityBar.background": "#F0FFFE",
        "activityBar.foreground": "#26BD51",
        "activityBar.inactiveForeground": "#5F9EA0",
        "activityBar.border": "#E0FFFF",
        "activityBarBadge.background": "#2E8B57",
        "activityBarBadge.foreground": "#FFFFFF",
        "sideBar.background": "#F1FBF7",
        "sideBar.foreground": "#3FA6A6",
        "sideBar.border": "#C5F9DA",
        "sideBarTitle.foreground": "#5AA32F",
        "sideBarSectionHeader.background": "#E0FFFF",
        "sideBarSectionHeader.foreground": "#2F4F4F",
        "list.activeSelectionBackground": "#2E8B5740",
        "list.activeSelectionForeground": "#2F4F4F",
        "list.inactiveSelectionBackground": "#2E8B5720",
        "list.hoverBackground": "#2E8B5730",
        "list.focusBackground": "#2E8B5740",
        "statusBar.background": "#F0FFFE",
        "statusBar.foreground": "#2F4F4F",
        "statusBar.border": "#E0FFFF",
        "statusBarItem.activeBackground": "#2E8B5740",
        "statusBarItem.hoverBackground": "#2E8B5730",
        "titleBar.activeBackground": "#F0FFFE",
        "titleBar.activeForeground": "#2F4F4F",
        "titleBar.inactiveBackground": "#F0FFFE",
        "titleBar.inactiveForeground": "#5F9EA0",
        "titleBar.border": "#E0FFFF",
        "tab.activeBackground": "#F0FFFE",
        "tab.activeForeground": "#2E8B57",
        "tab.inactiveBackground": "#F0FFFF",
        "tab.inactiveForeground": "#5F9EA0",
        "tab.border": "#E0FFFF",
        "tab.activeBorder": "#2E8B57",
        "panel.background": "#F0FFFF",
        "panel.border": "#E0FFFF",
        "panelTitle.activeForeground": "#2E8B57",
        "panelTitle.inactiveForeground": "#5F9EA0",
        "terminal.background": "#F0FFFE",
        "terminal.foreground": "#2F4F4F",
        "terminalCursor.background": "#F0FFFE",
        "terminalCursor.foreground": "#2E8B57"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#708090"
            }
        },
        {
            "name": "Identifiers",
            "scope": [
                "identifier",
                "variable.other",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#8FBC8F"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#1EAE72"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#3A7C7C"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#DC143C"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#22B26F"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#31A020"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#483D8B"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#483D8B"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#3FACCD"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#2EAAD0"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#A28CD2"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#6B8E23"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#00768B"
            }
        },
        {
            "name": "Entity Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#A220DA"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#4682B4"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#036F71"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#26DA20"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#4682B4"
            }
        }
    ]
}