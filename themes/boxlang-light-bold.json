{
    "name": "BoxLang Light (Bold)",
    "type": "light",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#FF4500",
        "variable.readonly": "#FF6347",
        "variable.local": "#FF7F50",
        "parameter": "#1E90FF",
        "function": "#0000CD",
        "method": "#0000CD",
        "class": "#8B008B",
        "interface": "#9932CC",
        "enum": "#DAA520",
        "namespace": "#228B22",
        "type": "#B8860B",
        "keyword": "#DC143C",
        "operator": "#FF4500",
        "string": "#008000",
        "number": "#FF1493",
        "boolean": "#FF6347",
        "comment": "#696969"
    },
    "colors": {
        "editor.background": "#FFFFFF",
        "editor.foreground": "#24292E",
        "editorLineNumber.foreground": "#6A737D",
        "editorLineNumber.activeForeground": "#FF4500",
        "editor.selectionBackground": "#FF450040",
        "editor.selectionHighlightBackground": "#FF450020",
        "editor.lineHighlightBackground": "#F6F8FA",
        "editorCursor.foreground": "#FF4500",
        "editorWhitespace.foreground": "#D1D5DA",
        "editorIndentGuide.background": "#E1E4E8",
        "editorIndentGuide.activeBackground": "#FF4500",
        "editor.findMatchBackground": "#FF450060",
        "editor.findMatchHighlightBackground": "#FF450030",
        "editor.wordHighlightBackground": "#FF450020",
        "editor.wordHighlightStrongBackground": "#FF450040",
        "editorBracketMatch.background": "#FF450040",
        "editorBracketMatch.border": "#FF4500",
        "editorBracketHighlight.foreground1": "#FF4500",
        "editorBracketHighlight.foreground2": "#7C5CFA",
        "editorBracketHighlight.foreground3": "#06B6D4",
        "editorBracketHighlight.foreground4": "#34D399",
        "activityBar.background": "#FFFFFF",
        "activityBar.foreground": "#24292E",
        "activityBar.inactiveForeground": "#6A737D",
        "activityBar.border": "#E1E4E8",
        "activityBarBadge.background": "#FF4500",
        "activityBarBadge.foreground": "#FFFFFF",
        "sideBar.background": "#F6F8FA",
        "sideBar.foreground": "#24292E",
        "sideBar.border": "#E1E4E8",
        "sideBarTitle.foreground": "#FF4500",
        "sideBarSectionHeader.background": "#E1E4E8",
        "sideBarSectionHeader.foreground": "#24292E",
        "list.activeSelectionBackground": "#FF450040",
        "list.activeSelectionForeground": "#24292E",
        "list.inactiveSelectionBackground": "#FF450020",
        "list.hoverBackground": "#FF450030",
        "list.focusBackground": "#FF450040",
        "statusBar.background": "#FFFFFF",
        "statusBar.foreground": "#24292E",
        "statusBar.border": "#E1E4E8",
        "statusBarItem.activeBackground": "#FF450040",
        "statusBarItem.hoverBackground": "#FF450030",
        "titleBar.activeBackground": "#FFFFFF",
        "titleBar.activeForeground": "#24292E",
        "titleBar.inactiveBackground": "#FFFFFF",
        "titleBar.inactiveForeground": "#6A737D",
        "titleBar.border": "#E1E4E8",
        "tab.activeBackground": "#FFFFFF",
        "tab.activeForeground": "#FF4500",
        "tab.inactiveBackground": "#F6F8FA",
        "tab.inactiveForeground": "#6A737D",
        "tab.border": "#E1E4E8",
        "tab.activeBorder": "#FF4500",
        "panel.background": "#F6F8FA",
        "panel.border": "#E1E4E8",
        "panelTitle.activeForeground": "#FF4500",
        "panelTitle.inactiveForeground": "#6A737D",
        "terminal.background": "#FFFFFF",
        "terminal.foreground": "#24292E",
        "terminalCursor.background": "#FFFFFF",
        "terminalCursor.foreground": "#FF4500"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#696969"
            }
        },
        {
            "name": "Identifiers",
            "scope": [
                "identifier",
                "variable.other",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#B8860B"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#FF4500"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#24292E"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#DC143C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#FF4500"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#0000CD"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#0000CD",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#FF4500"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#FF6347"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#FF1493"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#8B008B"
            }
        },
        {
            "name": "Entity Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#B8860B"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#1E90FF"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#8B008B"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#B8860B"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#1E90FF"
            }
        }
    ]
}