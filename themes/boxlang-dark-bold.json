{
    "name": "BoxLang Dark (Bold)",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "variable": "#FF6B35",
        "variable.readonly": "#FFB347",
        "variable.local": "#FF8C69",
        "parameter": "#87CEEB",
        "function": "#00CED1",
        "method": "#00CED1",
        "class": "#9370DB",
        "interface": "#DDA0DD",
        "enum": "#F0E68C",
        "namespace": "#98FB98",
        "type": "#FFD700",
        "keyword": "#FF1493",
        "operator": "#FF4500",
        "string": "#32CD32",
        "number": "#FF69B4",
        "boolean": "#FF6347",
        "comment": "#696969"
    },
    "colors": {
        "editor.background": "#0D1117",
        "editor.foreground": "#F8F8FF",
        "editorLineNumber.foreground": "#6E7681",
        "editorLineNumber.activeForeground": "#FF6B35",
        "editor.selectionBackground": "#FF6B3540",
        "editor.selectionHighlightBackground": "#FF6B3520",
        "editor.lineHighlightBackground": "#161B22",
        "editorCursor.foreground": "#FF6B35",
        "editorWhitespace.foreground": "#484F58",
        "editorIndentGuide.background": "#21262D",
        "editorIndentGuide.activeBackground": "#FF6B35",
        "editor.findMatchBackground": "#FF6B3560",
        "editor.findMatchHighlightBackground": "#FF6B3530",
        "editor.wordHighlightBackground": "#FF6B3520",
        "editor.wordHighlightStrongBackground": "#FF6B3540",
        "editorBracketMatch.background": "#FF6B3540",
        "editorBracketMatch.border": "#FF6B35",
        "activityBar.background": "#0D1117",
        "activityBar.foreground": "#F8F8FF",
        "activityBar.inactiveForeground": "#6E7681",
        "activityBar.border": "#21262D",
        "activityBarBadge.background": "#FF6B35",
        "activityBarBadge.foreground": "#FFFFFF",
        "sideBar.background": "#161B22",
        "sideBar.foreground": "#F8F8FF",
        "sideBar.border": "#21262D",
        "sideBarTitle.foreground": "#FF6B35",
        "sideBarSectionHeader.background": "#21262D",
        "sideBarSectionHeader.foreground": "#F8F8FF",
        "list.activeSelectionBackground": "#FF6B3540",
        "list.activeSelectionForeground": "#FFFFFF",
        "list.inactiveSelectionBackground": "#FF6B3520",
        "list.hoverBackground": "#FF6B3530",
        "list.focusBackground": "#FF6B3540",
        "statusBar.background": "#0D1117",
        "statusBar.foreground": "#F8F8FF",
        "statusBar.border": "#21262D",
        "statusBarItem.activeBackground": "#FF6B3540",
        "statusBarItem.hoverBackground": "#FF6B3530",
        "titleBar.activeBackground": "#0D1117",
        "titleBar.activeForeground": "#F8F8FF",
        "titleBar.inactiveBackground": "#0D1117",
        "titleBar.inactiveForeground": "#6E7681",
        "titleBar.border": "#21262D",
        "tab.activeBackground": "#161B22",
        "tab.activeForeground": "#FF6B35",
        "tab.inactiveBackground": "#0D1117",
        "tab.inactiveForeground": "#6E7681",
        "tab.border": "#21262D",
        "tab.activeBorder": "#FF6B35",
        "panel.background": "#161B22",
        "panel.border": "#21262D",
        "panelTitle.activeForeground": "#FF6B35",
        "panelTitle.inactiveForeground": "#6E7681",
        "terminal.background": "#0D1117",
        "terminal.foreground": "#F8F8FF",
        "terminalCursor.background": "#0D1117",
        "terminalCursor.foreground": "#FF6B35"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#696969"
            }
        },
        {
            "name": "Identifiers",
            "scope": [
                "identifier",
                "variable.other",
                "entity.name.variable",
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "string constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#FF6B35"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#FF1493",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#FF4500"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#00CED1"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#00CED1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": [
                "meta.block variable.other"
            ],
            "settings": {
                "foreground": "#FF6B35"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": [
                "support.other.variable",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#FFB347"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#FF69B4"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#32CD32",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#9370DB",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Entity Types",
            "scope": [
                "support.type"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#87CEEB"
            }
        },
        {
            "name": "Sub-methods",
            "scope": [
                "entity.name.module.js",
                "variable.import.parameter.js",
                "variable.other.class.js"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Language methods",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FF0000"
            }
        },
        {
            "name": "entity.name.method.js",
            "scope": [
                "entity.name.method.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#00CED1"
            }
        },
        {
            "name": "meta.method.js",
            "scope": [
                "meta.class-method.js entity.name.function.js",
                "variable.function.constructor"
            ],
            "settings": {
                "foreground": "#00CED1"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#9370DB"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FFD700"
            }
        },
        {
            "name": "CSS Classes",
            "scope": [
                "entity.other.attribute-name.class"
            ],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "name": "CSS ID's",
            "scope": [
                "source.sass keyword.control"
            ],
            "settings": {
                "foreground": "#00CED1"
            }
        },
        {
            "name": "Inserted",
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#32CD32"
            }
        },
        {
            "name": "Deleted",
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "Changed",
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#9370DB"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#87CEEB"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#87CEEB"
            }
        },
        {
            "name": "URL",
            "scope": [
                "*url*",
                "*link*",
                "*uri*"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#00CED1"
            }
        },
        {
            "name": "ES7 Bind Operator",
            "scope": [
                "source.js constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FF0000"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#9370DB"
            }
        },
        {
