.vscode/
.vscode-test/
src/
test/
*.test.js
*.spec.js

# Build and compilation artifacts
out/
dist/
*.tsbuildinfo
tsconfig.json
webpack.config.js
babel.config.*

# Node.js dependencies and package files
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.npm
.yarn-integrity

# Git files
.git/
.gitignore
.gitattributes

# Documentation and development files
docs/
examples/
*.md
!README.md
CONTRIBUTING.md
CODE_OF_CONDUCT.md
SECURITY.md

