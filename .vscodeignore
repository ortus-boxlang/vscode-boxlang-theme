scripts/
build/
.scripts/
gulpfile.js
gruntfile.js
rollup.config.js

# IDE and editor files
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Logs and temporary files
*.log
logs/
tmp/
temp/
.tmp/

# Environment and configuration files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
.npmrc
.yarnrc

# Testing files
coverage/
.nyc_output/
